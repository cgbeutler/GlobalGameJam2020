[sub_resource type="GDScript" id=1]
script/source = "extends Control

onready var logo := $Logo
onready var carpet := $Carpet
onready var sock := $Sock
onready var quit_button := $QuitButton

func _ready():
	if [\"Android\", \"iOS\", \"HTML5\", \"Server\"].has(OS.get_name()):
		quit_button.hide()

func _process(delta: float) -> void:
	__stepper += delta
	if __stepper > .03:
		__stepper = 0
		carpet.region_rect.position.x += 1
		carpet.region_rect.position.y += 1
		sock.region_rect.position.x += 1
		sock.region_rect.position.y += 1
	
	__stepper2 += delta
	if __stepper2 > .2:
		__stepper2 = 0
		logo.frame = ( logo.frame + 1 ) % 3
var __stepper := 0.0
var __stepper2 := 0.0


func _on_NewGameButton_pressed() -> void:
	get_tree().change_scene(\"res://Minigame.tscn\")

func _on_QuitButton_pressed() -> void:
	get_tree().quit()
"

